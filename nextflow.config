profiles {
  standard {
    process.executor = 'pbspro'
    process.module = ['Miniconda3/4.4.10']
    process.conda = "$baseDir/environment.yml"
    process.queue = 'short'
    process.memory = '64G'
    process.cpus = '16'
    process.time = '16h'
  }
  debug {
    //TODO clean up and merge to different config file
    process.executor = 'pbspro'
    process.module = ['Miniconda3/4.4.10']
    process.conda = "$baseDir/environment.yml"
    process.queue = 'debug'
    process.memory = '16G'
    process.cpus = '16'
    process.time = '0.5h'
  }
}
